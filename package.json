{
  "name": "react-semantic-ui-sortable-table-example",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "9.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gges5110/React-Semantic-UI-Sortable-Table-Example.git"
  },
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "build": "webpack --mode production",
    "build:watch": "webpack-dev-server --mode development --open --hot",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "now-build": "npm run build && mv public dist"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.11.1",
    "express": "^4.16.4",
    "json-server": "^0.14.0",
    "prop-types": "^15.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "semantic-ui-react": "^0.83.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-jest": "^23.4.2",
    "coveralls": "^3.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "fetch-mock": "^7.2.5",
    "jest": "^23.5.0",
    "react-test-renderer": "^16.4.2",
    "source-map-support": "^0.4.2",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "**/src/*.{jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
