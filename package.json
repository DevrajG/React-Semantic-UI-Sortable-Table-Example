    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "raw-loader": "^0.5.1",
    "semantic-ui-css": "^2.4.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "**/src/*.{jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
