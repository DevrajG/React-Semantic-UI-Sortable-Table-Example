{
  "name": "react-semantic-ui-sortable-table-example",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "9.2.0"
  },
  "main": "index.js",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "start": "nodemon -w server server/server.js",
    "mock-server": "json-server --watch ./server/vehicles.json --routes ./server/routes.json",
    "compile": "webpack",
    "watch": "webpack --watch",
    "build:watch": "webpack-dev-server --mode development --open --hot --inline",
    "heroku-postbuild": "npm install --dev && webpack --progress",
    "dev-all": "npm run watch & npm run watch-server & npm start",
    "test": "mocha --timeout 10000 --exit --check-leaks",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec --exit",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --exit && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "express": "^4.14.0",
    "body-parser": "^1.15.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "source-map-support": "^0.4.2"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.12",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "json-server": "^0.14.0",
    "mocha": "^4.1.0",
    "nodemon": "^1.12.1",
    "prop-types": "^15.6.0",
    "semantic-ui-react": "^0.77.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "whatwg-fetch": "^1.0.0"
  },
