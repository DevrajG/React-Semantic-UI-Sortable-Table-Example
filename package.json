{
  "name": "react-semantic-ui-sortable-table-example",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "9.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gges5110/React-Semantic-UI-Sortable-Table-Example.git"
  },
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
