{
  "public": true,
  "alias": "react-semantic-ui-sortable-table-example",
  "builds": [
    { "src": "server/server.js", "use": "@now/node-server" },
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {"src": "./app.bundle.js", "dest": "/static/app.bundle.js"},
    {"src": "./vendor.bundle.js", "dest": "/static/vendor.bundle.js"},
    { "src": "/api/v1/(.*)", "dest": "server/server.js" }
  ],
  "version": 2
}
